includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:
    scanFiles:
        - _ide_helper_models.php

    paths:
        - app/
        - database
        - resources
        - tests

    # Level 9 is the highest level
    level: 9

    ignoreErrors:

    excludePaths:

    checkMissingIterableValueType: false
    treatPhpDocTypesAsCertain: false
    reportUnmatchedIgnoredErrors: false
